<%= form_for @place do |f| %>
	<div> 
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>

	<div>
		<%= f.label :rating %>
		<%= f.text_field :rating %>
	</div>
	<div>
		<%= f.label :description %>
		<%= f.text_field :description %>
	</div>

	<div>
		<%= f.label :location %>
		<%= f.text_field :location %>
	</div>

	<%= f.submit %>
<% end %>