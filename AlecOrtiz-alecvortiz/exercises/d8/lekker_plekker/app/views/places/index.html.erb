<%= link_to "New Place", new_place_path %>

<h1> Cool Places </h1>

<table>
	<% @places.each do |place| %>
	<tr>
	<td><%= place.name %></td>
	<td><%= link_to 'Show', place_path(place) %></td>
	<td><%= link_to 'Edit', edit_place_path(place) %></td>
	<td><%= link_to 'Destroy', place_path(place),
					method: :delete,
					data: { confirm: 'Are you sure?' } %></td>
	</tr>
<% end %>
</table>